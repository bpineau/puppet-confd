[template]
src    = "<%= @src %>"
dest   = "<%= @dest %>"
<% if @owner -%>
owner  = "<%= @owner %>"
<% end -%>
<% if @group -%>
group  = "<%= @group %>"
<% end -%>
<% if @mode -%>
mode   = "<%= @mode %>"
<% end -%>
keys   = [
<% @keys.each do |key| -%>
  "<%= key %>",
<% end -%>
]
<% if @prefix -%>
prefix = "<%= @prefix %>"
<% end -%>
<% if @reload_cmd -%>
reload_cmd = "<%= @reload_cmd %>"
<% end -%>
<% if @check_cmd -%>
check_cmd  = "<%= @check_cmd %>"
<% end -%>